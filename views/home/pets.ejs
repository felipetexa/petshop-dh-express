<%- include('../partials/header') %>

<main>
  <section class="servicos-lista">
      <div class="container">
          <h1>Pets</h1>

          <% pets.forEach(pet => { %> 

          <article>
              <img class="thumb" src="<%= pet.imagem %>">
              <div class="info">
                  <h2><%= pet.nome %></h2>
                  <p><%= pet.especie %></p>
              </div>
          </article>
          <% }) %> 
          
      </div>
  </section>
</main>

<%- include('../partials/footer') %>